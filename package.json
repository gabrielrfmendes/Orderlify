{
	"name": "orderlify",
	"version": "1.0.0",
	"main": "expo/AppEntry.js",
	"scripts": {
		"start": "expo start",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"prettier:lint": "prettier --write . && eslint .",
		"test": "jest --watch --coverage=false --changedSince=origin/main",
		"testDebug": "jest -o --watch --coverage=false",
		"testFinal": "jest",
		"updateSnapshots": "jest -u --coverage=false"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"jest": {
		"preset": "jest-expo",
		"transformIgnorePatterns": [
			"node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|react-native-paper|react-native-svg)"
		],
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.{js,jsx,ts,tsx}",
			"!**/coverage/**",
			"!**/node_modules/**",
			"!**/babel.config.js",
			"!**/jest.setup.js"
		]
	},
	"dependencies": {
		"@expo/react-native-action-sheet": "^4.1.0",
		"@gorhom/bottom-sheet": "^4",
		"@react-native-async-storage/async-storage": "^1.23.1",
		"@react-native-community/datetimepicker": "8.0.1",
		"@react-navigation/material-top-tabs": "^6.6.13",
		"@react-navigation/native": "^6.1.17",
		"@react-navigation/native-stack": "^6.9.26",
		"axios": "^1.7.2",
		"expo": "~51.0.38",
		"expo-image-picker": "~15.0.7",
		"expo-status-bar": "~1.12.1",
		"react": "18.2.0",
		"react-native": "0.74.5",
		"react-native-circular-progress": "^1.4.0",
		"react-native-gesture-handler": "~2.16.1",
		"react-native-mask-input": "^1.2.3",
		"react-native-modal-datetime-picker": "^17.1.0",
		"react-native-pager-view": "6.3.0",
		"react-native-paper": "^5.12.3",
		"react-native-reanimated": "~3.10.1",
		"react-native-safe-area-context": "4.10.5",
		"react-native-screens": "3.31.1",
		"react-native-svg": "15.2.0",
		"react-native-tab-view": "^3.5.2",
		"react-native-toast-message": "^2.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@eslint/js": "^9.5.0",
		"@types/jest": "^29.5.12",
		"@types/react": "~18.2.45",
		"eslint": "^9.5.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-react": "^7.34.2",
		"globals": "^15.6.0",
		"husky": "^9.0.11",
		"jest": "^29.7.0",
		"jest-expo": "~51.0.4",
		"lint-staged": "^15.2.5",
		"prettier": "3.2.5",
		"typescript": "~5.3.3",
		"typescript-eslint": "^7.13.0"
	},
	"private": true,
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
